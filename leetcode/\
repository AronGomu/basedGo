package main

import "fmt"

func main() {
	twoSums([]int{2, 7, 11, 15}, 9)

}

func twoSums(nums []int, target int) []int {
	a := 0
	b := 1
	found := false

	for found != true {
		for i := b; i < len(nums)-1; i++ {
			fmt.Printf("a = %s, i = %s\n", a, i)
			fmt.Printf("%s + ")
			if nums[a]+nums[i] == target {
				return []int{a, i}
			}
		}
	}
}
